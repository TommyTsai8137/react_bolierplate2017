{
  "presets":[
    "react",
    [
      "env",
      {
        "targets": {
          "chrome": 58
        }, 
        "useBuiltIns": true
      }
    ],
    
  ],
  "plugins": [
    [
      "transform-class-properties", 
      { "spec": true }
    ],
    "transform-decorators-legacy",
    [
      "react-css-modules", 
      {
        webpackHotModuleReloading: true,
        handleMissingStyleName: "throw",
      },
    ]
  ]
}